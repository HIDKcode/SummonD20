{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=color-scheme]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "background-position: center"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not firefox <= 126",
    "not safari <= 17.5",
    "not and_ff <= 125",
    "not ios_saf <= 15.8"
  ]
}